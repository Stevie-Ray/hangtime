    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-vue": "^9.31.0",
    "jsdom": "^25.0.1",
    "npm-run-all2": "^7.0.1",
    "prettier": "^3.4.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.81.0",
    "typescript": "5.6.3",
    "vite": "^5.4.11",
    "vite-plugin-pwa": "^0.21.0",
    "vite-plugin-vue-devtools": "^7.6.5",
