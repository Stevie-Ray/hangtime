{
  "name": "hangtime",
  "version": "0.9.18",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "storybook": "start-storybook -s ./public -p 6006",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:e2e": "vue-cli-service test:e2e",
    "test:e2e:headless": "vue-cli-service test:e2e --headless",
    "test:unit": "vue-cli-service test:unit",
    "prettier:format-all": "prettier --write \"**/*.{vue,scss,sass,js,json}\"",
    "prettier:check": "prettier \"**/*.{vue,scss,sass,js,json}\" --list-different",
    "firebase:deploy:ci": "firebase deploy --non-interactive --token $FIREBASE_TOKEN",
    "bundlesize": "bundlesize"
  },
  "dependencies": {
    "@mdi/js": "^5.9.55",
    "chart.js": "^2.9.4",
    "core-js": "^3.16.0",
    "firebase": "^8.9.0",
    "ircra": "^2.2.4",
    "is_js": "^0.9.0",
    "js-video-url-parser": "^0.4.3",
    "lodash": "^4.17.21",
    "long-press-event": "^2.4.4",
    "nosleep.js": "^0.12.0",
    "pwacompat": "^2.0.17",
    "register-service-worker": "^1.7.2",
    "vue": "^2.6.14",
    "vue-chartjs": "^3.5.1",
    "vue-head": "^2.2.0",
    "vue-i18n": "^8.25.0",
    "vue-router": "^3.5.2",
    "vue-simple-svg": "^2.0.2",
    "vuedraggable": "^2.24.3",
    "vuetify": "^2.5.8",
    "vuex": "^3.6.2",
    "vuex-persistedstate": "^3.2.1"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.14.5",
    "@intlify/vue-i18n-loader": "^2.1.2",
    "@storybook/addon-a11y": "^6.3.6",
    "@storybook/addon-essentials": "^6.3.6",
    "@storybook/addon-links": "^6.3.6",
    "@storybook/addon-storysource": "^6.3.6",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/vue": "^6.3.6",
    "@types/lodash": "^4.14.172",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-e2e-cypress": "^4.5.13",
    "@vue/cli-plugin-eslint": "^4.5.13",
    "@vue/cli-plugin-pwa": "^4.5.13",
    "@vue/cli-plugin-unit-jest": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.2.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.6",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-require-context": "^0.1.1",
    "babel-preset-vue": "^2.0.2",
    "bundlesize": "^0.18.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.15.1",
    "firebase-tools": "^9.16.0",
    "prerender-spa-plugin": "^3.4.0",
    "sass": "1.32.13",
    "sass-loader": "^10.2.0",
    "vue-cli-plugin-i18n": "^2.1.2",
    "vue-cli-plugin-vuetify": "^2.4.2",
    "vue-storybook": "^1.2.0",
    "vue-template-compiler": "^2.6.14",
    "vuetify-loader": "^1.7.2",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-merge": "^5.8.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "vuetify/lib(.*)": "<rootDir>/node_modules/vuetify/es5$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/",
    "setupFilesAfterEnv": [
      "<rootDir>/tests/unit/setup.js"
    ]
  },
  "_id": "hangtime@1.0.0",
  "bundlesize": [
    {
      "path": "./dist/js/chunk-vendors*.js",
      "maxSize": "200 kB"
    },
    {
      "path": "./dist/js/chunk-firestore*.js",
      "maxSize": "100 kB"
    },
    {
      "path": "./dist/js/app*.js",
      "maxSize": "100 kB"
    },
    {
      "path": "./dist/js/client-chunk-*.js",
      "maxSize": "100 kB"
    }
  ],
  "readme": "README.md"
}
