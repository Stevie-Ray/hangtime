    "@mdi/js": "^7.4.47",
    "@unhead/vue": "^1.11.13",
    "@vueuse/core": "^11.3.0",
    "core-js": "^3.39.0",
    "firebase": "^11.0.2",
    "ircra": "^2.2.4",
    "js-video-url-parser": "^0.5.1",
    "pinia": "^2.2.7",
    "register-service-worker": "^1.7.2",
    "vue": "^3.5.12",
    "vue-gtag": "^2.0.1",
    "vue-i18n": "^10.0.4",
    "vue-inline-svg": "^3.1.4",
    "vue-router": "^4.5.0",
    "vuedraggable": "^4.1.0",
    "vuetify": "^3.7.4"
